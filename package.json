{
  "name": "ddlc-cp",
  "version": "1.0.0",
  "description": "ddlc-cp for posing characters to help developing DDLC mods!",
  "main": "index.js",
  "scripts": {
    "start": "electron ."
  },
  "repository": {
    "type": "git",
    "url": "ddlc-cp"
  },
  "author": "Catink123",
  "license": "ISC",
  "devDependencies": {
    "electron": "^3.1.8",
    "electron-builder": "^20.41.0",
    "jquery": "^3.3.1"
  },
  "dependencies": {
    "https": "^1.0.0",
    "npm": "^6.8.0",
    "npx": "^10.2.0",
    "resolve": "^1.10.0"
  },
  "build": {
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "snap",
      "executableName": "ddlc-cp",
      "icon": "build/icon.ico",
      "category": "Utility"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true
    },
    "extraResources": [
      "json/",
      "images/"
    ]
  }
}
